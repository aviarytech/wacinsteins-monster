{
  "name": "frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev2": "vite --mode dev2",
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "cy:headless": "cypress run --browser=firefox",
    "cy:debug": "DEBUG=cypress:cli cypress run",
    "cy:headed": "npx cypress run --headed --no-exit",
    "prettier": "prettier --write \"{cypress,src}/**/*.{ts,svelte,js,css,html}\""
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@cypress/code-coverage": "^3.9.10",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.22",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/parser": "^4.29.1",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "cssnano": "^5.0.6",
    "cypress": "^8.0.0",
    "cypress-log-to-output": "^1.1.2",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.6.3",
    "postcss": "^8.3.5",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "socket.io-client": "^4.2.0",
    "svelte": "^3.37.0",
    "svelte-check": "^2.1.0",
    "svelte-preprocess": "^4.7.4",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^26.5.6",
    "tslib": "^2.2.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "dependencies": {
    "@stablelib/sha256": "^1.0.1",
    "@tailwindcss/forms": "^0.3.3",
    "@transmute/credentials-context": "^0.7.0-unstable.7",
    "@transmute/did-context": "^0.7.0-unstable.7",
    "@transmute/security-context": "^0.7.0-unstable.7",
    "axios": "^0.21.3",
    "class-validator": "^0.13.1",
    "dexie": "^3.0.3",
    "jdenticon": "^3.1.1",
    "jsonld": "^5.2.0",
    "magic-sdk": "^4.4.1",
    "qr-scanner": "^1.3.0",
    "qrious": "^4.0.2",
    "schema-dts": "^0.10.0",
    "svelte-closable": "^0.0.0",
    "svelte-copy-to-clipboard": "^0.2.5",
    "svelte-jester": "^1.7.0",
    "svelte-navigator": "^3.1.5",
    "svelte-tooltip": "^1.2.0",
    "sweetalert": "^2.1.2",
    "timeago-simple": "^1.2.4"
  },
  "jest": {
    "rootDir": "src",
    "verbose": true,
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "setupFilesAfterEnv": [
    "@testing-library/jest-dom/extend-expect"
  ]
}
